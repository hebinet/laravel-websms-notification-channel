{
  "name": "hebinet/laravel-websms-notification-channel",
  "description": "WebSMS Notification Channel for laravel.",
  "keywords": [
    "laravel",
    "notifications",
    "websms"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Hebenstreit",
      "email": "office@hebinet.at",
      "homepage": "https://www.hebinet.at",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/notifications": "^8.0|^9.0",
    "hebinet/websmscom-php": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "mockery/mockery": "^1.0",
    "pestphp/pest": "^1.21",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Hebinet\\Notifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hebinet\\Tests\\Notifications\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "fix-style": "vendor/bin/php-cs-fixer fix"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hebinet\\Notifications\\WebSmsChannelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
